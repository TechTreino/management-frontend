<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
					http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="AcademiaDigital\Model\Entity\MuscleGroup" table="muscle_groups">
        <id name="id" type="integer"><generator strategy="AUTO" /></id>
        <field name="name" column="name" length="50" type="string" />
        <field name="createdAt" column="created_at" type="datetime" />
        <field name="modifiedAt" column="modified_at" type="datetime" />
    </entity>
</doctrine-mapping>